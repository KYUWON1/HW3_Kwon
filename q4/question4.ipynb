    "    \n",
    "def fac(x):\n",
    "    if(x == 0 or x == 1):\n",
    "        return 1\n",
    "    else:\n",
    "        return x*fac(x-1)\n",
    "        \n",
    "for i in range(0,15,2):\n",
    "    print(\"input is {0:d}! output is {1:d} \".format(i,fac(i)))\n",
    "    i +=2"
